<%= link_to "Create New User", new_user_path %>

<%if @users.present? %>
  <p>Below is the list of users</p>
  <table width="100%">
    <tr>
      <td>ID</td>
      <td>first_name</td>
      <td>last_name</td>
      <td>email</td>
      <td>Action</td>
    </tr>
    <%@users.each do |u|%>
    <tr>
      <td><%=u%></td>
      
      
    </tr>
   <%end%>
  </table>
<%else%>
  <p>No User is Found</p>
<%end%>


